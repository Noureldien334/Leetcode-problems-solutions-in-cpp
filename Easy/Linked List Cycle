/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    bool hasCycle(ListNode *head) {
        while(head)
        {
            if(head->val==-12342234)
                return true;
            head->val=-12342234;
            head=head->next;
            
        }
        return false;
    }
};
