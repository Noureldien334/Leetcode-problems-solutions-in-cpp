//Topic(s) (Arrays, Prefix sum)
class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        vector<int> sum(nums.size());
        int total = 0;
        for(int i = 0; i < nums.size(); i++){
            total += nums[i];
            sum[i] = total;
        }
        return sum;
    }
};
