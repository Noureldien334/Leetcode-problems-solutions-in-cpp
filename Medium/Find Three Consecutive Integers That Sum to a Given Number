class Solution {
public:
    vector<long long> sumOfThree(long long num) {
        long long x;
        vector <long long> nums;
        if(num%3==0)
        {
            nums.push_back((num/3)-1);
            nums.push_back((num/3));
            nums.push_back((num/3)+1);
            
        }
        return nums;
    }
};
